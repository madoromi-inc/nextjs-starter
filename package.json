{
  "name": "nextjs-starter",
  "version": "1.0.0",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "format": "npm run format:ts && npm run format:misc",
    "format:ts": "eslint --fix \"**/*.{js,ts,tsx}\"",
    "format:misc": "prettier --write \"**/*.{css,json}\"",
    "lint": "eslint \"**/!(*.d).{js,ts,tsx}\"",
    "type-check": "tsc"
  },
  "dependencies": {
    "autoprefixer": "^10.2.5",
    "next": "^10.0.9",
    "postcss": "^8.2.8",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "tailwindcss": "^2.0.4",
    "tailwindcss-aspect-ratio": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^12.12.21",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.2.1",
    "typescript": "4.0"
  },
  "license": "MIT"
}
